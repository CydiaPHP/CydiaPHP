{
	"name": "wapmorgan/unified-archive",
	"description": "UnifiedArchive - unified interface to archive (zip, 7z, rar, gz, bz2, xz, cab, tar, tar.gz, tar.bz2, tar.x, tar.Z, iso) for listing, reading, extracting and creation + built-in console archive manager + fully implemented PclZip-like interface (create, listContent, extract, properties, add, delete, merge, duplicate).",
	"keywords": ["archive", "compression", "rar", "7z", "zip", "gzip", "bzip2", "tar", "lzma2", "cab", "ncompress", "iso"],
	"license": "MIT",
	"authors": [
		{
			"name": "wapmorgan",
			"email": "wapmorgan@gmail.com",
			"role": "developer"
		}
	],
	"require": {
		"php": ">=5.4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8"
	},
	"replace": {
		"wapmorgan/cam": "1.0.2"
	},
	"suggest": {
		"ext-zip": "ZIP support",
		"ext-rar": "RAR support",
		"ext-phar": "TAR support",
		"ext-zlib": "GZ and TAR.GZ support",
		"ext-bz2": "BZ2 and TAR.BZ2 support",
		"ext-xz": "XZ and TAR.XZ support",
		"pear/archive_tar": "another TAR support",
		"bin-ncompress": "TAR.Z support",
		"gemorroj/archive7z": "7ZIP support",
		"wapmorgan/cab-archive": "CAB support",
		"phpclasses/php-iso-file": "ISO support",
		"docopt/docopt": "Requirement for cam (Console Archive Manager)"
	},
	"autoload": {
		"psr-4": {
			"wapmorgan\\UnifiedArchive\\": "src/"
		},
		"files": [
			"src/function.gzip_stat.php",
			"src/stream.compress.lzw.php"
		]
	},
	"bin": ["bin/cam"],
	"minimum-stability": "dev"
}
